{
  "name": "swiss-vault",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx vite build && npx esbuild server/index-prod.ts --platform=node --bundle --minify --format=esm --outfile=dist/server.js",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx vite\" \"npx tsx watch server/index.ts\""
  },
  "dependencies": {
    "vite": "^5.4.8",
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "@neondatabase/serverless": "^0.9.5",
    "drizzle-orm": "^0.33.0",
    "dotenv": "^16.4.5",
    "tailwindcss": "^3.4.13",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47"
  },
  "devDependencies": {
    "esbuild": "^0.21.5",
    "typescript": "^5.5.4",
    "tsx": "^4.19.1",
    "concurrently": "^8.2.2",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.7.4"
  }
}
